@page "{key}"
@model IStichIt.Pages.DetailsModel
@{
}


<!-- Page Content -->
<div class="container">

    <!-- Item Heading -->
    <h1 class="my-4">
        Page Heading
        <small>Secondary Text</small>
    </h1>

    <!-- Item Row -->
    <div class="row">

        <div class="col-md-8">
            <h3 class="my-3">Title</h3>
            <p>Description</p>
            <h3 class="my-3">Location</h3>
            <ul>
                <li>Address.City</li>
                <li>Address.State</li>
                <li>ClubCategory</li>
            </ul>
        </div>

        <div class="col-md-4">
            <img class="img-fluid" src="" alt="">
        </div>

    </div>
    <!-- /.row -->
    <!-- Before and After Pictures -->
    <h3 class="my-4">Before and After</h3>

    <div class="row">

        <div class="col-md-3 col-sm-6 mb-4">
            <a href="#">
                <img class="img-fluid" src="https://via.placeholder.com/500x300" alt="">
            </a>
        </div>

        <div class="col-md-3 col-sm-6 mb-4">
            <a href="#">
                <img class="img-fluid" src="https://via.placeholder.com/500x300" alt="">
            </a>
        </div>

        <div class="col-md-3 col-sm-6 mb-4">
            <a href="#">
                <img class="img-fluid" src="https://via.placeholder.com/500x300" alt="">
            </a>
        </div>

        <div class="col-md-3 col-sm-6 mb-4">
            <a href="#">
                <img class="img-fluid" src="https://via.placeholder.com/500x300" alt="">
            </a>
        </div>

    </div>
    <!-- /.row -->

</div>
<!-- /.container -->